{
    "data": [
        {
            "id": "$ev_plat4_0010_0005_blt_0",
            "text": "",
            "speaker": ""
        },
        {
            "id": "$ev_plat4_0010_0010_blt_0",
            "text": "ここを抜けりゃ　伍番魔晄炉だ",
            "speaker": "バレット"
        },
        {
            "id": "$ev_plat4_0010_0020_tif_0",
            "text": "思ってたより　大変そう",
            "speaker": "ティファ"
        },
        {
            "id": "$ev_plat4_0010_0030_blt_0",
            "text": "まあ　なんとかなるだろ",
            "speaker": "バレット"
        },
        {
            "id": "$ev_plat4_0010_0030_blt_1",
            "text": "あれが　伍番魔晄炉の柱だ",
            "speaker": "バレット"
        },
        {
            "id": "$ev_plat4_0010_0040_cld_0",
            "text": "道順は？",
            "speaker": "クラウド"
        },
        {
            "id": "$ev_plat4_0010_0050_blt_0",
            "text": "プランＥの次は　Ｆ　Ｇ　Ｈってな",
            "speaker": "バレット"
        },
        {
            "id": "$ev_plat4_0010_0060_cld_0",
            "text": "続きがあったのか？",
            "speaker": "クラウド"
        },
        {
            "id": "$ev_plat4_0010_0070_tif_0",
            "text": "Ｆは　この区画の名前",
            "speaker": "ティファ"
        },
        {
            "id": "$ev_plat4_0010_0080_blt_0",
            "text": "Ｇを抜けて　Ｈまで向かう",
            "speaker": "バレット"
        },
        {
            "id": "$ev_plat4_0010_0090_tif_0",
            "text": "Ｈにあるリフトで\r\n魔晄炉の近くまで行けるの",
            "speaker": "ティファ"
        },
        {
            "id": "$ev_plat4_0010_0090_tif_1",
            "text": "ビッグスも　その先で待ってるはず",
            "speaker": "ティファ"
        },
        {
            "id": "$ev_plat4_0010_0100_tif_0",
            "text": "もう　日が傾きはじめてるね",
            "speaker": "ティファ"
        },
        {
            "id": "$ev_plat4_0010_0110_blt_0",
            "text": "おう　行くぞ！",
            "speaker": "バレット"
        },
        {
            "id": "$ev_plat4_0010_0110_blt_1",
            "text": "神羅に泡ふかせてやる",
            "speaker": "バレット"
        },
        {
            "id": "$ev_plat4_0010_0120_cld_0",
            "text": "",
            "speaker": ""
        },
        {
            "id": "$ev_plat4_0010_0130_tif_0",
            "text": "",
            "speaker": ""
        },
        {
            "id": "$ev_plat4_0035_0030_ana_0",
            "text": "非常用電力の供給を確認\r\nゲートの再起動を開始します",
            "speaker": "端末音声"
        },
        {
            "id": "$ev_plat4_0035_0040_blt_0",
            "text": "おっしゃ　上手くいったな",
            "speaker": "バレット"
        },
        {
            "id": "$ev_plat4_0035_0045_tif_0",
            "text": "",
            "speaker": ""
        },
        {
            "id": "$ev_plat4_0035_0050_tif_0",
            "text": "ありがとう　クラウド",
            "speaker": "ティファ"
        },
        {
            "id": "$ev_plat4_0035_0060_cld_0",
            "text": "",
            "speaker": ""
        },
        {
            "id": "$ev_plat4_0090_0010_blt_0",
            "text": "間違いねえ\r\nこいつがビッグスの言ってた　メインリフトだ",
            "speaker": "バレット"
        },
        {
            "id": "$ev_plat4_0090_0010_blt_1",
            "text": "あいつも　この先で待ってるぜ",
            "speaker": "バレット"
        },
        {
            "id": "$ev_plat4_0090_0020_ana_0",
            "text": "電力供給が不足しています",
            "speaker": "端末音声"
        },
        {
            "id": "$ev_plat4_0090_0030_blt_0",
            "text": "またかよ",
            "speaker": "バレット"
        },
        {
            "id": "$ev_plat4_0090_0030_cld_0",
            "text": "ライト３つ分　必要だな",
            "speaker": "クラウド"
        },
        {
            "id": "$ev_plat4_0090_0040_tif_0",
            "text": "そんなに……",
            "speaker": "ティファ"
        },
        {
            "id": "$ev_plat4_0090_0060_tif_0",
            "text": "この辺りの　全部ってこと？",
            "speaker": "ティファ"
        },
        {
            "id": "$ev_plat4_0090_0080_blt_0",
            "text": "こうなりゃ　片っ端だ",
            "speaker": "バレット"
        },
        {
            "id": "$ev_plat4_0090_0100_tif_0",
            "text": "迷ったら　ここに戻ってこよう",
            "speaker": "ティファ"
        },
        {
            "id": "$ev_plat4_0090_0100_tif_1",
            "text": "『Ｈ－０１』の番号が　目印",
            "speaker": "ティファ"
        },
        {
            "id": "$ev_plat4_0090_0110_cld_0",
            "text": "ああ",
            "speaker": "クラウド"
        },
        {
            "id": "$ev_plat4_0090_0130_blt_0",
            "text": "こっからいちばん近いライトっつーと",
            "speaker": "バレット"
        },
        {
            "id": "$ev_plat4_0090_0150_cld_0",
            "text": "あれだな",
            "speaker": "クラウド"
        },
        {
            "id": "$ev_plat4_0200_0001_cld_0",
            "text": "",
            "speaker": ""
        },
        {
            "id": "$ev_plat4_0200_0002_cld_0",
            "text": "",
            "speaker": ""
        },
        {
            "id": "$ev_plat4_0200_0003_big_0",
            "text": "",
            "speaker": ""
        },
        {
            "id": "$ev_plat4_0200_0010_big_0",
            "text": "降参！",
            "speaker": "ビッグス"
        },
        {
            "id": "$ev_plat4_0200_0020_big_0",
            "text": "てか　クラウド？",
            "speaker": "ビッグス"
        },
        {
            "id": "$ev_plat4_0200_0020_big_1",
            "text": "ジェシーと　ウェッジは？",
            "speaker": "ビッグス"
        },
        {
            "id": "$ev_plat4_0200_0025_cld_0",
            "text": "",
            "speaker": ""
        },
        {
            "id": "$ev_plat4_0200_0030_blt_0",
            "text": "どんな感じだ？",
            "speaker": "バレット"
        },
        {
            "id": "$ev_plat4_0200_0040_big_0",
            "text": "犯人グループが列車から飛び降りたって\r\n上は大騒ぎ",
            "speaker": "ビッグス"
        },
        {
            "id": "$ev_plat4_0200_0040_big_1",
            "text": "おかげで　ここは静かでな",
            "speaker": "ビッグス"
        },
        {
            "id": "$ev_plat4_0200_0040_big_2",
            "text": "魔晄炉へのルートは　ささっと確保済みよ",
            "speaker": "ビッグス"
        },
        {
            "id": "$ev_plat4_0200_0050_blt_0",
            "text": "ヘイヘイヘイ　ビーッグス！",
            "speaker": "バレット"
        },
        {
            "id": "$ev_plat4_0200_0055_big_0",
            "text": "",
            "speaker": ""
        },
        {
            "id": "$ev_plat4_0200_0060_big_0",
            "text": "で　あとのふたりは？",
            "speaker": "ビッグス"
        },
        {
            "id": "$ev_plat4_0200_0070_tif_0",
            "text": "ジェシーがね　ケガをしちゃって",
            "speaker": "ティファ"
        },
        {
            "id": "$ev_plat4_0200_0080_big_0",
            "text": "ひどいのか？",
            "speaker": "ビッグス"
        },
        {
            "id": "$ev_plat4_0200_0090_blt_0",
            "text": "口数は減ってねえ　大丈夫だ",
            "speaker": "バレット"
        },
        {
            "id": "$ev_plat4_0200_0100_big_0",
            "text": "世話になるな",
            "speaker": "ビッグス"
        },
        {
            "id": "$ev_plat4_0200_0110_cld_0",
            "text": "仕事だ",
            "speaker": "クラウド"
        },
        {
            "id": "$ev_plat4_0200_0120_big_0",
            "text": "四番街駅行きに乗って　正解だったな",
            "speaker": "ビッグス"
        },
        {
            "id": "$ev_plat4_0200_0120_big_1",
            "text": "でも　こっちの本命は伍番",
            "speaker": "ビッグス"
        },
        {
            "id": "$ev_plat4_0200_0120_big_2",
            "text": "警備は　四番に集中してる",
            "speaker": "ビッグス"
        },
        {
            "id": "$ev_plat4_0200_0130_big_0",
            "text": "ちょっと複雑だが\r\nこの奥はもう　魔晄炉ン中だ",
            "speaker": "ビッグス"
        },
        {
            "id": "$ev_plat4_0200_0140_blt_0",
            "text": "せまっちい　穴ポコだぜ",
            "speaker": "バレット"
        },
        {
            "id": "$ev_plat4_0200_0150_big_0",
            "text": "文句言うなよ",
            "speaker": "ビッグス"
        },
        {
            "id": "$ev_plat4_0200_0150_big_1",
            "text": "こっちは　補給物資とワイヤーリール",
            "speaker": "ビッグス"
        },
        {
            "id": "$ev_plat4_0200_0160_big_0",
            "text": "調子は？",
            "speaker": "ビッグス"
        },
        {
            "id": "$ev_plat4_0200_0170_blt_0",
            "text": "これ以上ねえってくらい　上々よ",
            "speaker": "バレット"
        },
        {
            "id": "$ev_plat4_0200_0180_tif_0",
            "text": "みんなのこと　お願い",
            "speaker": "ティファ"
        },
        {
            "id": "$ev_plat4_0200_0190_big_0",
            "text": "おうよ",
            "speaker": "ビッグス"
        },
        {
            "id": "$ev_plat4_0200_0200_big_0",
            "text": "",
            "speaker": ""
        },
        {
            "id": "$ev_plat4_0200_0230_big_0",
            "text": "",
            "speaker": ""
        },
        {
            "id": "$ev_plat4_0200_0240_big_0",
            "text": "",
            "speaker": ""
        },
        {
            "id": "$ev_plat4_0200_0250_blt_0",
            "text": "よし　オレたちも　ワイヤーの装備だ",
            "speaker": "バレット"
        },
        {
            "id": "$ev_plat4_0200_0255_tif_0",
            "text": "",
            "speaker": ""
        },
        {
            "id": "$ev_plat4_0200_0260_blt_0",
            "text": "落とさねえように\r\nしっかりくくりつけとけよ",
            "speaker": "バレット"
        },
        {
            "id": "$ev_plat4_0200_0260_blt_1",
            "text": "作戦が終わったら\r\nこいつで一気に　下まで降りる",
            "speaker": "バレット"
        },
        {
            "id": "$ev_plat4_0200_0270_blt_0",
            "text": "生きて帰んぞ",
            "speaker": "バレット"
        },
        {
            "id": "$ev_plat4_0200_0280_cld_0",
            "text": "当然だ",
            "speaker": "クラウド"
        },
        {
            "id": "$ev_plat4_0200_0290_tif_0",
            "text": "",
            "speaker": ""
        },
        {
            "id": "$pv_plat4_0015_0010_blt_0",
            "text": "扉の先がＧか",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0020_0010_tif_0",
            "text": "これで　開くかな？",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0021_0010_cld_0",
            "text": "",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0021_0021_tif_0",
            "text": "",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0021_0030_tif_0",
            "text": "ダメ？",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0021_0040_cld_0",
            "text": "電力がきてない",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0021_0050_blt_0",
            "text": "あそこは　電気ついてるぜ",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0021_0070_cld_0",
            "text": "制御室か",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0021_0071_tif_0",
            "text": "",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0021_0080_tif_0",
            "text": "行ってみよう",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0022_0010_cld_0",
            "text": "まずは電力だな",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0023_0010_blt_0",
            "text": "おい　そっちになんかあんのか",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0025_0010_cld_0",
            "text": "ライトのスイッチか",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0026_0010_tif_0",
            "text": "扉　開かなかった\r\n電力不足みたいで",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0026_0020_blt_0",
            "text": "どうにかしねえとな",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0030_0010_blt_0",
            "text": "かろうじて生きてっか",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0030_0020_tif_0",
            "text": "動くかな",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0032_0020_ana_0",
            "text": "電力供給が不足しています",
            "speaker": "アナウンス"
        },
        {
            "id": "$pv_plat4_0032_0030_cld_0",
            "text": "ダメだな",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0032_0040_tif_0",
            "text": "ねえ　これ",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0032_0050_tif_0",
            "text": "電力不足時の対処法だって",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0032_0060_cld_0",
            "text": "ライトを消せば\r\n非常用電力を確保できるのか",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0032_0070_tif_0",
            "text": "ライトって　もしかして『スラムの太陽』？",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0032_0080_tif_0",
            "text": "スラムを照らすライトのことを\r\nそう呼んでるの",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0032_0090_blt_0",
            "text": "そいつを消せば　扉が開くってワケか",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0032_0100_tif_0",
            "text": "でも　太陽が消えたら困るよね\r\n四番街の人たち",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0032_0110_blt_0",
            "text": "魔晄炉をぶっ壊したら\r\nどっちにしたって　消えちまう",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0032_0120_tif_0",
            "text": "そうだったね",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0032_0130_cld_0",
            "text": "戻るぞ",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0034_0010_tif_0",
            "text": "あった\r\nこうして見ると　大きいね",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0034_0020_blt_0",
            "text": "スイッチは　はしごの上か\r\nけっこう高えな",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0034_0030_cld_0",
            "text": "俺が行こう",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0034_0040_tif_0",
            "text": "うん　下で待ってるね",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0036_0010_cld_0",
            "text": "",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0040_0020_blt_0",
            "text": "これで　Ｇまでは達成だな",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0040_0030_tif_0",
            "text": "うん　あとは魔晄炉を目印に――",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0040_0050_tif_0",
            "text": "このマユ……",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0040_0060_blt_0",
            "text": "やつらの巣穴へようこそって感じか　こりゃ",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0040_0070_blt_0",
            "text": "どこまで腐ってやがんだ　このピザはよ",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0050_0010_cld_0",
            "text": "",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0050_0030_tif_0",
            "text": "大丈夫？",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0050_0040_cld_0",
            "text": "ああ",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0050_0050_blt_0",
            "text": "マテリアに　気ぃ取られてたろ",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0050_0060_tif_0",
            "text": "すぐそこなのに",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0060_0010_tif_0",
            "text": "もう一度　確認しない？",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0061_0030_blt_0",
            "text": "今どの辺だ　こりゃ",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0061_0040_tif_0",
            "text": "私たちの目指す魔晄炉は　Ｈの先",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0061_0050_cld_0",
            "text": "ああ",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0061_0060_tif_0",
            "text": "でも　ここまではＨにつながってる道\r\nなかったよね",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0061_0070_blt_0",
            "text": "まあ　歩き回ってりゃそのうち……",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0061_0080_cld_0",
            "text": "あんたひとりで　やってくれ",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0061_0100_tif_0",
            "text": "ほかに　つながってそうな場所ってなると",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0061_0120_cld_0",
            "text": "壁沿いか",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0061_0130_tif_0",
            "text": "ダメもとで行ってみない？\r\n目的地があると　気もまぎれるし",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0061_0140_cld_0",
            "text": "悪くない案だ",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0061_0150_blt_0",
            "text": "あのでけえ扇風機が目印か\r\n確かに　あれ見て歩けば迷わねえな",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0070_0010_tif_0",
            "text": "見て\r\nあのパイプの上　登れるかも",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0071_0020_cld_0",
            "text": "つたって行けそうだな",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0071_0030_blt_0",
            "text": "マジか",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0073_0010_blt_0",
            "text": "なんか　すげえファンの音が……",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0073_0020_cld_0",
            "text": "怖じ気づいたか",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0073_0040_blt_0",
            "text": "誰が！",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0073_0050_blt_0",
            "text": "てめえの細っこい体が\r\n吹き飛ばされねぇかって　こっちは――",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0073_0060_tif_0",
            "text": "もう　急いでるんでしょ",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0073_0080_blt_0",
            "text": "おう　ついてこい",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0075_0010_blt_0",
            "text": "こわくねえ　こわくねえ……",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0080_0010_blt_0",
            "text": "いい風　吹いてんな",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0080_0020_tif_0",
            "text": "全然　よくない",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0082_0010_tif_0",
            "text": "よそ見　ダメだから",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0082_0020_blt_0",
            "text": "おう",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0083_0010_tif_0",
            "text": "あのファンの中　なんだろう",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0084_0010_cld_0",
            "text": "あれは――",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0085_0010_blt_0",
            "text": "バカやろう！",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0085_0020_blt_0",
            "text": "よそ見しちまったじゃねぇか",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0085_0030_cld_0",
            "text": "",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0086_0010_tif_0",
            "text": "なんとか渡りきったね",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0086_0020_blt_0",
            "text": "寿命ちぢんだぜ",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0086_0030_blt_0",
            "text": "ほんのちょっとだけな\r\n２秒だ　２秒",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0086_0040_cld_0",
            "text": "",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0100_0020_tif_0",
            "text": "止まった？",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0100_0040_ana_0",
            "text": "換気システム　メンテナンスモードに移行",
            "speaker": "アナウンス"
        },
        {
            "id": "$pv_plat4_0100_0050_ana_0",
            "text": "制限時間内に\r\nロック解除用端末にアクセスしてください",
            "speaker": "アナウンス"
        },
        {
            "id": "$pv_plat4_0100_0070_blt_0",
            "text": "なにしてんだ　おい",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0102_0010_ana_0",
            "text": "認証エラー　認証エラー\r\n制限時間内にアクセスが確認できませんでした",
            "speaker": "アナウンス"
        },
        {
            "id": "$pv_plat4_0102_0020_ana_0",
            "text": "換気システムを　再稼働します",
            "speaker": "アナウンス"
        },
        {
            "id": "$pv_plat4_0103_0010_cld_0",
            "text": "ファンの制御装置……",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0103_0015_cld_0",
            "text": "",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0104_0010_ana_0",
            "text": "ロック解除禁止\r\n現在　換気システムが稼働中です",
            "speaker": "アナウンス"
        },
        {
            "id": "$pv_plat4_0105_0010_ana_0",
            "text": "ロック解除　アクセスを許可します",
            "speaker": "アナウンス"
        },
        {
            "id": "$pv_plat4_0107_0010_blt_0",
            "text": "なんだ　どこ行くんだ\r\nお宝探しに出発ってか～",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0108_0010_ana_0",
            "text": "換気システムのメンテナンスモードを設定します\r\n作業者は　端末にアクセスしてください",
            "speaker": "アナウンス"
        },
        {
            "id": "$pv_plat4_0110_0020_blt_0",
            "text": "召喚マテリアか！",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0110_0030_tif_0",
            "text": "きれい",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0110_0040_blt_0",
            "text": "どんなやつが飛び出すんだ　おい",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0110_0050_cld_0",
            "text": "さあな",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0140_0040_tif_0",
            "text": "まずは　１つ目",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0140_0050_ana_0",
            "text": "電力回復　パワーオン",
            "speaker": "アナウンス"
        },
        {
            "id": "$pv_plat4_0140_0060_blt_0",
            "text": "残りは　あの２つだな",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0140_0070_ana_0",
            "text": "侵入者撃退システム　再起動開始",
            "speaker": "アナウンス"
        },
        {
            "id": "$pv_plat4_0140_0071_ana_0",
            "text": "",
            "speaker": "アナウンス"
        },
        {
            "id": "$pv_plat4_0140_0100_tif_0",
            "text": "撃退システム？",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0140_0110_cld_0",
            "text": "電力が復旧したか",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0140_0120_blt_0",
            "text": "それがどうした\r\n全部ぶっ壊して魔晄炉へ向かう",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0150_0030_blt_0",
            "text": "これで　２つ目か",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0150_0050_tif_0",
            "text": "あとは　あのライトだけだね",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0150_0060_blt_0",
            "text": "終わりが見えてきたな",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0150_0080_cld_0",
            "text": "ここから　下に降りられそうだ",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0160_0030_tif_0",
            "text": "これで　３つ目\r\nメインリフト　動かせるようになったかな",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0160_0040_cld_0",
            "text": "たぶんな",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0160_0050_blt_0",
            "text": "よし\r\n『Ｈー０１』エリアまで　戻るぞ",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0170_0010_ana_0",
            "text": "非常用電力の供給を確認\r\nリフトの再起動を開始します",
            "speaker": "アナウンス"
        },
        {
            "id": "$pv_plat4_0170_0030_ana_0",
            "text": "再起動完了\r\nリフトに搭乗してください",
            "speaker": "アナウンス"
        },
        {
            "id": "$pv_plat4_0170_0050_blt_0",
            "text": "あばよ　鉄くず迷路",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0171_0010_blt_0",
            "text": "そういや　マテリアが落っこちてたよな\r\n拾っとかなくていいのか",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0171_0020_sys_0",
            "text": "拾っとかなくていいのか？",
            "speaker": ""
        },
        {
            "id": "$pv_plat4_0172_0010_cld_0",
            "text": "そうだな　戻るか",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0173_0010_cld_0",
            "text": "いや　先を急ごう",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0173_0020_cld_0",
            "text": "",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0174_0010_cld_0",
            "text": "",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0174_0020_cld_0",
            "text": "",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0175_0010_cld_0",
            "text": "そうだな　戻るか",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0175_0020_cld_0",
            "text": "いや　先を急ごう",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0180_0010_tif_0",
            "text": "いよいよ　魔晄炉か",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0180_0020_blt_0",
            "text": "暗くて　陰気くせえ\r\n星の命が削られる場所だ",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0180_0030_tif_0",
            "text": "",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0180_0040_blt_0",
            "text": "中は魔晄のにおいが鼻につくぞ\r\n覚悟しとけ",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0180_0050_tif_0",
            "text": "うん",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0180_0060_cld_0",
            "text": "",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0190_0010_blt_0",
            "text": "もうすぐか",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0190_0020_tif_0",
            "text": "",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0190_0030_cld_0",
            "text": "ティファ？",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_plat4_0190_0040_tif_0",
            "text": "ううん　いよいよだなって",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0190_0050_tif_0",
            "text": "思ってたより　緊張してたみたい",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_plat4_0190_0060_blt_0",
            "text": "こればっかりは　オレも慣れねえ",
            "speaker": "バレット"
        },
        {
            "id": "$pv_plat4_0190_0070_tif_0",
            "text": "うん",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0500_0010_blt_0",
            "text": "下が丸見えじゃねぇか",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0500_0020_cld_0",
            "text": "黙って進め",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_plat4_0500_0030_tif_0",
            "text": "下見なければ　平気だよ",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0500_0040_blt_0",
            "text": "案外ずぶてえな　おまえら",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0510_0010_tif_0",
            "text": "あの扉　使ってないのかな",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0510_0020_blt_0",
            "text": "ってか　この通路自体\r\n使ってるやつがいるとは思えねえ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0510_0030_tif_0",
            "text": "これじゃ　点検もできないよね",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0520_0010_tif_0",
            "text": "スラムの太陽のせいで\r\n電力不足なのかな　ここ",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0520_0020_blt_0",
            "text": "さあな\r\n点検をサボってるだけじゃねぇか",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0520_0030_tif_0",
            "text": "明るいのは便利だけど\r\nたくさん魔晄を　消費してるんだね",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0530_0010_blt_0",
            "text": "さっそく　歓迎してくれるってよ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0530_0020_cld_0",
            "text": "気を抜くなよ",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_plat4_0540_0010_cld_0",
            "text": "ここは　渡れないな",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_plat4_0540_0020_tif_0",
            "text": "進める方へ行くしかないね",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0550_0010_tif_0",
            "text": "故障中？",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0550_0020_cld_0",
            "text": "こっちも　電力不足らしい",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_plat4_0550_0030_blt_0",
            "text": "ちょうどいいじゃねえか\r\n侵入し放題ってことだろ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0560_0010_blt_0",
            "text": "くせえガスを　スラムに垂れ流しやがって",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0560_0020_tif_0",
            "text": "ガスのとこ　モンスターが集まってる？",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0560_0030_blt_0",
            "text": "トンネルん中といっしょだ\r\n神羅と魔晄のせいってな",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0570_0010_tif_0",
            "text": "さっきの通路もダメか",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0570_0020_blt_0",
            "text": "まあ　どっかは繋がってんだろ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0570_0030_tif_0",
            "text": "地道に探すしかないね",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0570_0040_blt_0",
            "text": "あのマテリアの入手経路もな",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0570_0050_cld_0",
            "text": "",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_plat4_0580_0010_blt_0",
            "text": "こういう場所はな\r\n迷うわけねえって信じるのが大事だ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0580_0020_tif_0",
            "text": "それ　絵本のセリフだよね",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0580_0030_blt_0",
            "text": "おう　マリンが気に入っててよ\r\nマネしてやると喜ぶんだ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0580_0040_tif_0",
            "text": "『人生は終わりのない迷路だぜ』でしょ？\r\n私も覚えちゃった",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0590_0010_tif_0",
            "text": "あれって　換気ファンかな",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0590_0020_blt_0",
            "text": "吹き飛ばしても\r\nガスが消えてなくなるわけじゃねえ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0590_0030_blt_0",
            "text": "やばいもんは　全部スラムに垂れ流しよ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0600_0010_tif_0",
            "text": "もう目の前がＨ\r\n足もと　気をつけて",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0600_0020_cld_0",
            "text": "あれが　目的のリフトか",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_plat4_0600_0030_tif_0",
            "text": "メインリフト\r\nたしか　そう言ってた！",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0610_0010_blt_0",
            "text": "見ろ　魔晄炉が呼んでるぜ\r\n早く来い　早く来いってな！",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0620_0010_tif_0",
            "text": "『暗いスラムを見守るあかりは』",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0620_0020_tif_0",
            "text": "『未来までも照らす道しるべであり\r\n　太陽なのです』",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0620_0030_blt_0",
            "text": "なんだそりゃ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0620_0040_tif_0",
            "text": "スラムの太陽の宣伝文句\r\nプレジデントが演説してた",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0620_0050_blt_0",
            "text": "神羅に照らされる未来なんざ\r\nお先真っ暗に決まってらあ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0630_0010_tif_0",
            "text": "あったよ　スイッチ",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0630_0020_blt_0",
            "text": "さくっと消しちまうか",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0640_0010_ana_0",
            "text": "侵入者発見　掃討モードへ移行します",
            "speaker": "アナウンス"
        },
        {
            "id": "$tk_plat4_0640_0020_cld_0",
            "text": "中距離射撃タイプだな",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_plat4_0640_0030_blt_0",
            "text": "あいつらは　オレにまかせとけ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0641_0020_ana_0",
            "text": "",
            "speaker": "アナウンス"
        },
        {
            "id": "$tk_plat4_0650_0010_tif_0",
            "text": "先が思いやられるな",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0650_0020_cld_0",
            "text": "復旧したのは　警備兵器だけじゃない",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_plat4_0660_0010_blt_0",
            "text": "ここも　崩れてんのかよ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0660_0020_tif_0",
            "text": "ほかの方法　探さないとだね",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0661_0010_blt_0",
            "text": "どこにつなげるか　よく考えろよ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0662_0020_tif_0",
            "text": "通路が動かせるんだ",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0663_0010_blt_0",
            "text": "上から来たか",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0664_0010_blt_0",
            "text": "ぴったりじゃねえか",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0667_0010_blt_0",
            "text": "ライトまでの架け橋　しっかりつなげろよ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0670_0010_blt_0",
            "text": "次のライトは　どっちだ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0670_0020_tif_0",
            "text": "こまめに確認しないと\r\nわかんなくなっちゃうね",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0670_0030_blt_0",
            "text": "バカみてぇにでかくて　助かるぜ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0680_0010_blt_0",
            "text": "崩れねえよな",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0680_0020_cld_0",
            "text": "下は　四番街スラムか",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_plat4_0680_0030_tif_0",
            "text": "興味ある？\r\n今度　案内しよっか",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0680_0040_blt_0",
            "text": "少しは　心配しろよ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0682_0010_tif_0",
            "text": "動きそうだよ",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0684_0010_blt_0",
            "text": "ここにも　リフトがあんな",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0690_0010_tif_0",
            "text": "たくさん消しちゃったね　太陽",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0690_0020_blt_0",
            "text": "太陽なんて\r\n本物がひとつありゃ　じゅうぶんだ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0690_0030_blt_0",
            "text": "神羅に作れんのは　ニセモンだけよ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0700_0020_tif_0",
            "text": "ぐるっとまわって　戻ってきたみたい",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0700_0030_tif_0",
            "text": "えっと　最後のライトは……",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0730_0010_tif_0",
            "text": "あのライトで最後\r\nずいぶん　まわり道しちゃったね",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0730_0020_blt_0",
            "text": "ここには　仲間の目印もないしな\r\nジェシーとウェッジがいないのも痛え",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0730_0030_tif_0",
            "text": "ふたりのこと　ビッグスになんて言おう",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0730_0040_tif_0",
            "text": "きっと心配するよね",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0740_0010_tif_0",
            "text": "ジェシーのケガ　大丈夫かな",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0740_0020_blt_0",
            "text": "あいつらのぶんまで\r\n神羅をこらしめてやんのが　いちばんの治療だ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0740_0030_tif_0",
            "text": "そうだね\r\nクラウドはウェッジのぶん　お願い",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0740_0040_cld_0",
            "text": "",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_plat4_0741_0010_blt_0",
            "text": "ここにきて　迷子かよ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0742_0010_tif_0",
            "text": "早く　『Ｈー０１』まで戻ろう",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0743_0010_tif_0",
            "text": "メインリフトって　こっちだっけ",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0745_0010_blt_0",
            "text": "迷ってるわけじゃねえよな",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0746_0010_blt_0",
            "text": "寄り道か？\r\nま　つきあうぜ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0747_0010_blt_0",
            "text": "なんだ　どこ行くんだ\r\nお宝探しに出発ってか～",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0750_0010_cld_0",
            "text": "ビッグスとの合流場所は",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_plat4_0750_0020_blt_0",
            "text": "魔晄炉へ続く　通路ん中だ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0750_0030_tif_0",
            "text": "暇つぶしのプラン\r\nＡからＺまであるって言ってた",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0750_0040_blt_0",
            "text": "Ｇくらいまでには　着くようにしねえとな",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0760_0010_tif_0",
            "text": "メインリフトまで　戻ってきたね",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0760_0020_blt_0",
            "text": "首を洗って待ってろよ　魔晄炉",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0770_0010_blt_0",
            "text": "ビッグスとのランデブーポイントは　この先だ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0770_0020_tif_0",
            "text": "遅くなったから　きっと心配してるね",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0780_0010_tif_0",
            "text": "派手に暴れたのに　誰も追ってこないね",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0780_0020_cld_0",
            "text": "ミッドガルは　神羅の手の内だ\r\n監視はされていただろう",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_plat4_0780_0030_tif_0",
            "text": "気づいてたなら言ってよ",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0780_0040_cld_0",
            "text": "言ったら　やめてたか",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_plat4_0780_0050_blt_0",
            "text": "最初から　ギリギリの綱渡りだ\r\n渡りきるしかねえ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_0780_0060_tif_0",
            "text": "力づくでも",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0780_0070_cld_0",
            "text": "得意そうだ",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_plat4_0780_0080_tif_0",
            "text": "どうでしょう",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_0930_0010_cld_0",
            "text": "行くぞ",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_plat4_0930_0020_blt_0",
            "text": "なんだ？",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_1000_0010_tif_0",
            "text": "ここ　さっきも来た？",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_1010_0010_tif_0",
            "text": "この辺り　もう見たよね",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_1020_0010_tif_0",
            "text": "迷ってる？",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_1100_0010_tif_0",
            "text": "このリフト　さっき使ったよね",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_1100_0020_blt_0",
            "text": "なんか　忘れもんか",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_1110_0010_blt_0",
            "text": "こりゃ　さっきも乗ったやつか",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_1110_0020_tif_0",
            "text": "迷ってるわけじゃないよね",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_1120_0010_tif_0",
            "text": "あれ　戻るの？",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_1120_0020_blt_0",
            "text": "用があるなら　早くしろよ",
            "speaker": "バレット"
        },
        {
            "id": "$tk_plat4_1360_0010_cld_0",
            "text": "ライト　３つぶんの電力か",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_plat4_1370_0010_cld_0",
            "text": "ライトを３つ消せば動きそうだ",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_plat4_1380_0010_cld_0",
            "text": "電力不足で動きそうにない",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_plat4_1460_0010_cld_0",
            "text": "ライト２つぶん　不足してるな",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_plat4_1470_0010_cld_0",
            "text": "必要なのは　ライト２つぶんか",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_plat4_1480_0010_cld_0",
            "text": "電力不足だな",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_plat4_1560_0010_cld_0",
            "text": "必要な電力は　ライト１つぶんか",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_plat4_1570_0010_cld_0",
            "text": "ライトを１つ消せば　動きそうだ",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_plat4_1580_0010_cld_0",
            "text": "足りないのは　ライト１つぶんだな",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_plat4_1800_0010_ana_0",
            "text": "電力供給が不足しています",
            "speaker": "アナウンス"
        },
        {
            "id": "$tk_plat4_1800_0020_cld_0",
            "text": "ライトを消すのが先だな",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_plat4_1810_0010_ana_0",
            "text": "電力供給が不足しています",
            "speaker": "アナウンス"
        },
        {
            "id": "$tk_plat4_1810_0020_tif_0",
            "text": "あれを消せば　動くかな？",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_plat4_1820_0010_ana_0",
            "text": "電力供給が不足しています",
            "speaker": "アナウンス"
        },
        {
            "id": "$tk_plat4_1820_0020_tif_0",
            "text": "まずは　ライトを消さないとだね",
            "speaker": "ティファ"
        }
    ],
