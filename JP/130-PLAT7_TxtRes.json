        {
            "id": "$ev_plat7_0100_0050_ss1_0",
            "text": "見つけた　至急応援をよこしてくれ",
            "speaker": "警備兵"
        },
        {
            "id": "$ev_plat7_0100_0060_blt_0",
            "text": "ど派手な登場になっちまったな",
            "speaker": "バレット"
