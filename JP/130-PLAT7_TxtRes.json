            "speaker": "ティファ"
        },
        {
            "id": "$ev_plat7_0120_0015_tif_0",
            "text": "",
            "speaker": ""
        },
        {
            "id": "$ev_plat7_0120_0020_blt_0",
            "text": "また　店やろうぜ",
            "speaker": "バレット"
        },
        {
            "id": "$ev_plat7_0120_0030_tif_0",
            "text": "うん　やろう",
            "speaker": "ティファ"
