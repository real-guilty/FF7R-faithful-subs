            "text": "",
            "speaker": ""
        },
        {
            "id": "$ev_grav7_8170_0060_tif_0",
            "text": "あの子",
            "speaker": "ティファ"
        },
        {
            "id": "$ev_grav7_8170_0070_tif_0",
            "text": "すごい",
            "speaker": "ティファ"
        },
        {
            "id": "$ev_grav7_8170_0080_art_0",
            "text": "またね",
            "speaker": "エアリス"
        },
        {
            "id": "$level_grav7_sys_turnrail",
            "text": "転車台が稼働した。",
            "info": ""
        },
        {
            "id": "$lv_grav7_0430_0050_art_0",
            "text": "みーつけた",
            "speaker": "エアリス"
        },
        {
            "id": "$lv_grav7_0460_0010_tif_0",
            "text": "ここにもいるかな？",
            "speaker": "ティファ"
        },
        {
            "id": "$lv_grav7_0460_0020_art_0",
            "text": "みたい",
            "speaker": "エアリス"
        },
        {
            "id": "$lv_grav7_0460_0030_art_0",
            "text": "みーーっけっ！",
            "speaker": "エアリス"
        },
        {
            "id": "$pv_grav7_0030_0010_cld_0",
            "text": "まるで　迷路だな",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_grav7_0030_0020_tif_0",
            "text": "通称　列車墓場\r\nジャンクパーツの宝庫",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_grav7_0030_0030_tif_0",
            "text": "あそこに見えてる車両倉庫の向こうが\r\n七番街のはずだよ",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_grav7_0030_0040_tif_0",
            "text": "あかりのついている場所を　通っていこう",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_grav7_0030_0050_art_0",
            "text": "あかり　あかり……",
            "speaker": "エアリス"
        },
        {
            "id": "$pv_grav7_0030_0060_cld_0",
            "text": "あれだな",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_grav7_0050_0010_art_0",
            "text": "ひと　全然いないね",
            "speaker": "エアリス"
        },
        {
            "id": "$pv_grav7_0050_0020_tif_0",
            "text": "普段は　誰も近寄らないの",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_grav7_0050_0030_tif_0",
            "text": "モンスターだけじゃなくて\r\n怖い噂もあるし",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_grav7_0050_0040_art_0",
            "text": "うわさ？",
            "speaker": "エアリス"
        },
        {
            "id": "$pv_grav7_0050_0050_tif_0",
            "text": "列車墓場には　オバケが出るって",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_grav7_0050_0060_tif_0",
            "text": "夜の列車墓場に迷い込むと\r\n二度と戻ってこられなくなるんだって",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_grav7_0050_0070_art_0",
            "text": "",
            "speaker": "エアリス"
        },
        {
            "id": "$pv_grav7_0050_0080_cld_0",
            "text": "オバケ",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_grav7_0050_0090_tif_0",
            "text": "子供だましだって思うよ　私も\r\nでも――",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_grav7_0050_0100_cld_0",
            "text": "早く抜けよう",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_grav7_0120_0010_tif_0",
            "text": "うわ　なになに？",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_grav7_0121_0002_dl02_0",
            "text": "",
            "speaker": "ダミーアクタ"
        },
        {
            "id": "$pv_grav7_0125_0020_tif_0",
            "text": "もう　びっくりさせないで",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_grav7_0130_0010_tif_0",
            "text": "さっき\r\n子供の声　聞こえたよね？",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_grav7_0130_0020_cld_0",
            "text": "子供がうろつく時間じゃない",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_grav7_0130_0030_tif_0",
            "text": "じゃあ――",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_grav7_0161_0002_dl02_0",
            "text": "",
            "speaker": "ダミーアクタ"
        },
        {
            "id": "$pv_grav7_0181_0002_dl02_0",
            "text": "",
            "speaker": "ダミーアクタ"
        },
        {
            "id": "$pv_grav7_0241_0001_dl02_0",
            "text": "",
            "speaker": "ダミーアクタ"
        },
        {
            "id": "$pv_grav7_0290_0010_cld_0",
            "text": "ここが　車両倉庫か",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_grav7_0320_0010_tif_0",
            "text": "なに？",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_grav7_0320_0020_art_0",
            "text": "あっ　そこにも",
            "speaker": "エアリス"
        },
        {
            "id": "$pv_grav7_0321_0001_dl02_0",
            "text": "",
            "speaker": "ダミーアクタ"
        },
        {
            "id": "$pv_grav7_0350_0010_art_0",
            "text": "きみは……",
            "speaker": "エアリス"
        },
        {
            "id": "$pv_grav7_0370_0020_tif_0",
            "text": "どうしよう",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_grav7_0370_0030_gan_0",
            "text": "この列車は……駅経由……墓場行",
            "speaker": "？？？？"
        },
        {
            "id": "$pv_grav7_0370_0040_gan_0",
            "text": "ご乗車になる……は\r\nお急……そ　ぎ――",
            "speaker": "？？？？"
        },
        {
            "id": "$pv_grav7_0370_0050_gan_0",
            "text": "",
            "speaker": "？？？？"
        },
        {
            "id": "$pv_grav7_0375_0020_tif_0",
            "text": "どうしよう",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_grav7_0375_0030_gan_0",
            "text": "この列車は……駅経由……墓場行",
            "speaker": "？？？？"
        },
        {
            "id": "$pv_grav7_0375_0040_gan_0",
            "text": "ご乗車になる……は\r\nお急……そ　ぎ――",
            "speaker": "？？？？"
        },
        {
            "id": "$pv_grav7_0375_0050_gan_0",
            "text": "",
            "speaker": "？？？？"
        },
        {
            "id": "$pv_grav7_0380_0040_art_0",
            "text": "いじわる～",
            "speaker": "エアリス"
        },
        {
            "id": "$pv_grav7_0380_0050_cld_0",
            "text": "クレーンは　上か",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_grav7_0381_0021_dl02_0",
            "text": "",
            "speaker": "ダミーアクタ"
        },
        {
            "id": "$pv_grav7_0400_0010_cld_0",
            "text": "ダメだな　電源が落ちている",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_grav7_0400_0020_art_0",
            "text": "これもイタズラかな\r\n電源　どこかで操作してる？",
            "speaker": "エアリス"
        },
        {
            "id": "$pv_grav7_0401_0011_dl02_0",
            "text": "",
            "speaker": "ダミーアクタ"
        },
        {
            "id": "$pv_grav7_0411_0001_dl02_0",
            "text": "",
            "speaker": "ダミーアクタ"
        },
        {
            "id": "$pv_grav7_0621_0010_tif_0",
            "text": "よかった　動いたね",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_grav7_0621_0020_art_0",
            "text": "通れるようになったかな",
            "speaker": "エアリス"
        },
        {
            "id": "$pv_grav7_0621_0030_cld_0",
            "text": "行ってみよう",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_grav7_0695_0010_art_0",
            "text": "回ってる！",
            "speaker": "エアリス"
        },
        {
            "id": "$pv_grav7_0695_0020_tif_0",
            "text": "向こう側に行けるかな",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_grav7_0760_0010_tif_0",
            "text": "また道が塞がれてる\r\nあと少しのはずなのに",
            "speaker": "ティファ"
        },
        {
            "id": "$pv_grav7_0760_0020_art_0",
            "text": "列車　動かせないかな",
            "speaker": "エアリス"
        },
        {
            "id": "$pv_grav7_0760_0030_cld_0",
            "text": "調べてみよう",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_grav7_0765_0010_cld_0",
            "text": "読みが当たったな　まだ動きそうだ",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_grav7_0765_0030_art_0",
            "text": "やったね",
            "speaker": "エアリス"
        },
        {
            "id": "$pv_grav7_0830_0010_cld_0",
            "text": "神羅の増援か",
            "speaker": "クラウド"
        },
        {
            "id": "$pv_grav7_0830_cld_0",
            "text": "",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_grav7_0041_0001_dl02_0",
            "text": "",
            "speaker": "ダミーアクタ"
        },
        {
            "id": "$tk_grav7_0091_0001_dl02_0",
            "text": "",
            "speaker": "ダミーアクタ"
        },
        {
            "id": "$tk_grav7_0130_0010_tif_0",
            "text": "反対側にも扉　あったみたい\r\nぐるっと回り込むしかなさそうだね",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_grav7_0180_0010_tif_0",
            "text": "『列車墓場に迷い込むと\r\n　二度と戻ってこられない』か……",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_grav7_0180_0020_tif_0",
            "text": "捕まってたら　危なかったのかな",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_grav7_0180_0030_art_0",
            "text": "捕まってるのは　あの子たちかも",
            "speaker": "エアリス"
        },
        {
            "id": "$tk_grav7_0180_0040_cld_0",
            "text": "その話は　もう――",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_grav7_0180_0050_art_0",
            "text": "帰れない原因　それだったら？",
            "speaker": "エアリス"
        },
        {
            "id": "$tk_grav7_0200_0010_cld_0",
            "text": "ティファ　平気か？",
            "speaker": "クラウド"
        },
        {
            "id": "$tk_grav7_0200_0020_tif_0",
            "text": "うん　さあ行こう",
            "speaker": "ティファ"
        },
        {
            "id": "$tk_grav7_0260_0010_tif_0",
            "text": "このあたりの車両　まだ動くかもしれない",
            "speaker": "ティファ"
        }
    ],
