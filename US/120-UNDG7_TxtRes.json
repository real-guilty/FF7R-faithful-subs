        },
        {
            "id": "$ev_undg7_1050_0045_np13_0",
            "text": "",
            "speaker": ""
        },
        {
            "id": "$ev_undg7_1050_0050_tif_0",
            "text": "Marle!",
            "speaker": "Tifa"
        },
        {
            "id": "$ev_undg7_1050_0060_mar_0",
            "text": "You're alive!",
